$assert OP in ["ABS", "NEG", "SQR"]
#include <assert.h>

#include <riscv_vector.h>

#include "src/xnnpack/common.h"
#include "src/xnnpack/vunary.h"


$VFOP_V_F16 = {
$  "ABS": "__riscv_vfabs_v_f16m%d",
$  "NEG": "__riscv_vfneg_v_f16m%d",
$  "SQR": "__riscv_vfmul_vv_f16m%d",
$}[OP] % LMUL
void xnn_f16_v${OP.lower()}_ukernel__rvvfp16arith_u${LMUL}v(
    size_t batch,
    const xnn_float16* input,
    xnn_float16* output,
    const struct xnn_f16_default_params* unused_params)
{
  assert(batch != 0);
  assert(batch % sizeof(uint16_t) == 0);
  assert(input != NULL);
  assert(output != NULL);

  const uint16_t* i = (const uint16_t*) input;
  uint16_t* o = (uint16_t*) output;

  batch >>= XNN_LOG2_SIZEOF_FLOAT16;
  do {
    const size_t n = __riscv_vsetvl_e16m${LMUL}(batch);
    const vfloat16m${LMUL}_t vi = __riscv_vle16_v_f16m${LMUL}((const void *) i, n);
    i += n;
    $if OP == "SQR":
      const vfloat16m${LMUL}_t vo = __riscv_vfmul_vv_f16m${LMUL}(vi, vi, n);
    $else:
      const vfloat16m${LMUL}_t vo = ${VFOP_V_F16}(vi, n);
    __riscv_vse16_v_f16m${LMUL}((void *) o, vo, n);
    o += n;

    batch -= n;
  } while (batch != 0);
}
