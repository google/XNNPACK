    }

    i0 = (const __fp16*) ((uintptr_t) i${ROW_TILE} - input_decrement);
    i1 = (const __fp16*) ((uintptr_t) i${ROW_TILE+1} - input_decrement);
    $for M in range(2, 4 + ROW_TILE):
