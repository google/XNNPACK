$assert NR % 16 == 0
$assert NR <= 64

#include <assert.h>
#include <stddef.h>
#include <stdint.h>

#include <immintrin.h>

#include "xnnpack/intrinsics-polyfill.h"
#include "xnnpack/packw.h"
$if PREFETCH:
  #include "xnnpack/prefetch.h"


void xnn_x32_packw_gemm_gio_ukernel_x${NR}__avx512f_u${KBLOCK}${"_prfm" if PREFETCH else ""}(
  size_t g,
  size_t nc,
  size_t kc,
  size_t nr,
  size_t kr,
  size_t sr,
  size_t k_stride,
  const uint32_t* weights,
  const uint32_t* bias,
  const void* scale,
  uint32_t* packed_weights,
  size_t extra_bytes,
  const void* params)
{
  assert(g != 0);
  assert(nc != 0);
  assert(kc != 0);
  assert(nr == ${NR});   // This kernel is for NR=${NR}
  assert(kr == 1);
  assert(sr == 1);
  assert(k_stride != 0);
  assert(weights != NULL);
  assert(packed_weights != NULL);

  const __m512 vzero = _mm512_setzero_ps();
  const float* b = (const float*) bias;
  float* packed_w = (float*) packed_weights;
  do {
    // NC main loop multiple of ${NR}
    const float* w = (const float*) weights;
    size_t n = nc;

    for (; n >= ${NR}; n -= ${NR}) {
      if XNN_LIKELY(b != NULL) {
        $for N in range(0,NR,16):
          const __m512 vb${N//16} = _mm512_loadu_ps(b + ${N});
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, vb${N//16});
        b += ${NR};
      } else {
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, vzero);
      }
      packed_w += ${NR};

      // KC main loop ${KBLOCK}x${NR}
      size_t k = kc;
      $if KBLOCK > 1:
        for (; k >= ${KBLOCK}; k -= ${KBLOCK}) {
          $for K in range(KBLOCK):
            $for N in range(0,NR,16):
              const __m512 v${N//16}_${K} = _mm512_loadu_ps(w + ${N} + ${K} * k_stride);
          $if PREFETCH:
            $for K in range(KBLOCK):
              $for N in range(0,NR,16):
                xnn_prefetch_to_l1((const int8_t*) w + ${N*4+960});
          $for K in range(KBLOCK):
            $for N in range(0,NR,16):
              _mm512_store_ps(packed_w + ${N+K*NR}, v${N//16}_${K});
          w += k_stride * ${KBLOCK};
          packed_w += ${NR*KBLOCK};
        }

      // KC remainder loop
      for (; k > 0; --k) {
        $for N in range(0,NR,16):
          const __m512 v${N//16} = _mm512_loadu_ps(w + ${N});
        $if PREFETCH:
          $for N in range(0,NR,16):
            xnn_prefetch_to_l1((const int8_t*) w + ${N*4+960});
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, v${N//16});
        w += k_stride;
        packed_w += ${NR};
      }
      w = w - kc * k_stride + ${NR};  // Advance to next column of ${NR} floats
    }

    // NC remainder (1..${NR-1})
    if XNN_UNLIKELY(n != 0) {
      assert(n >= 1);
      assert(n <= ${NR-1});

      // Prepare mask for valid 32-bit elements (depends on n).
      $for N in range(0,NR,16):
        const __mmask16 vmask${N//16} = _cvtu32_mask16((uint32_t) (((UINT64_C(1) << n) - 1) >> ${N}));

      if XNN_LIKELY(b != NULL) {
        $for N in range(0,NR,16):
          const __m512 vb${N//16} = _mm512_maskz_loadu_ps(vmask${N//16}, b + ${N});
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, vb${N//16});
        b += n;
      } else {
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, vzero);
      }
      packed_w += ${NR};

      // KC main loop
      for (size_t k = kc; k > 0; --k) {
        $for N in range(0,NR,16):
          const __m512 v${N//16} = _mm512_maskz_loadu_ps(vmask${N//16}, w + ${N});
        $for N in range(0,NR,16):
          _mm512_store_ps(packed_w + ${N}, v${N//16});
        w += k_stride;
        packed_w += ${NR};
      }
    }
    weights += nc * kc;
  } while (--g != 0);
}
