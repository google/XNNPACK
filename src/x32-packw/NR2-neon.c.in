$assert NR >= 2
$assert KUNROLL >= 2

#include <assert.h>
#include <stddef.h>
#include <stdint.h>

#include <arm_neon.h>

#include <xnnpack/math.h>
#include <xnnpack/packw.h>


void xnn_x32_packw_gemm_goi_ukernel_x${NR}__neon(
  size_t g,
  size_t nc,
  size_t kc,
  size_t nr,
  size_t kr,
  size_t sr,
  const uint32_t* weights,
  const uint32_t* bias,
  uint32_t* packed_weights,
  size_t extra_bytes,
  const void* params)
{
  assert(g != 0);
  assert(nc != 0);
  assert(kc != 0);
  assert(nr == ${NR});   // This kernel is for NR=${NR}
  assert(kr == 1);
  assert(sr == 1);
  assert(weights != NULL);
  assert(packed_weights != NULL);

  $for N in range(0,NR,2):
    $for K in range(0,KUNROLL,2):
      uint32x2x2_t v${N}${K};

  do {
    // NC main loop multiple of ${NR}
    const uint32_t* w = weights;
    size_t n = nc;
    if XNN_LIKELY(n >= ${NR}) {
      do {
        if XNN_LIKELY(bias != NULL) {
          $for N in range(0,NR,2):
            uint32x2_t vb${N} = vld1_u32(bias + ${N});
          $for N in range(0,NR,2):
            vst1_u32(packed_weights + ${N}, vb${N});
          bias += ${NR};
        }
        packed_weights += ${NR};

        const uint32_t* w0 = w;
        $for N in range(1, NR):
          const uint32_t* w${N} = w${N-1} + kc;

        // KC main loop multiple of ${NR}x${KUNROLL}
        size_t k = kc;
        for (; k >= ${KUNROLL}; k -= ${KUNROLL}) {
          $for N in range(0,NR,2):
            $for NN in range(0,2):
              $for K in range(0,KUNROLL,2):
                $if NN == 0:
                  v${N}${K} = vld2_dup_u32(w${N+NN} + ${K});
                $else:
                  v${N}${K} = vld2_lane_u32(w${N+NN} + ${K}, v${N}${K}, ${NN});
              w${N+NN} += ${KUNROLL};
          $for K in range(0,KUNROLL,2):
            $for NN in range(0,2):
              $for N in range(0,NR,2):
                vst1_u32(packed_weights + ${N+NN*NR+K*KUNROLL}, v${N}${K}.val[${NN}]);
          packed_weights += ${NR*KUNROLL};
        }

        // KC remainder
        for (; k != 0; --k) {
          $for N in range(0,NR,2):
            $for NN in range(0,2):
              $for K in range(0,KUNROLL,2):
                $if NN == 0:
                  v${N}${K}.val[0] = vld1_dup_u32(w${N+NN} + ${K});
                $else:
                  v${N}${K}.val[0] = vld1_lane_u32(w${N+NN} + ${K}, v${N}${K}.val[0], ${NN});
              w${N+NN} += ${KUNROLL//2};
          $for K in range(0,KUNROLL,2):
            $for N in range(0,NR,2):
              vst1_u32(packed_weights + ${N+K*KUNROLL}, v${N}${K}.val[0]);
          packed_weights += ${NR*KUNROLL//2};
        }
        packed_weights = (uint32_t*) ((uintptr_t) packed_weights + extra_bytes);
        w = w${NR-1};
        n -= ${NR};
      } while (n >= ${NR});
    }

    if XNN_UNLIKELY(n != 0) {
      $if NR == 2:
        // NC remainder of 1
        if XNN_LIKELY(bias != NULL) {
          *packed_weights = *bias++;
        }
        packed_weights += ${NR};
        size_t k = kc;
        do {
          *packed_weights = *w++;
          packed_weights += ${NR};
        } while (--k);
        packed_weights = (uint32_t*) ((uintptr_t) packed_weights + extra_bytes);
      $else:
        // NC remainder (1..${NR-1})
        if XNN_LIKELY(bias != NULL) {
          size_t nb = n;
          do {
            *packed_weights++  = *bias++;
          } while (--nb != 0);
          packed_weights += (${NR} - n);
        } else {
          packed_weights += ${NR};
        }

        size_t k = kc;
        do {
          const uint32_t* wn = w;
          size_t nw = n;
          do {
            *packed_weights++ = wn[0];
            wn += kc;
          } while (--nw != 0);
          ++w;
          packed_weights += (${NR} - n);
        } while (--k);
        packed_weights = (uint32_t*) ((uintptr_t) packed_weights + extra_bytes);
    }
    weights += nc * kc;
  } while (--g != 0);
}
