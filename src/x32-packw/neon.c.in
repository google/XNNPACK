$assert NR >= 4
$assert KUNROLL >= 4

#include <assert.h>
#include <stddef.h>
#include <stdint.h>
#include <string.h>
#if defined(_MSC_VER)
  #include <malloc.h>
#else
  #include <alloca.h>
#endif
#include <arm_neon.h>

#include <xnnpack/math.h>
#include <xnnpack/packw.h>


void xnn_x32_packw_gemm_goi_ukernel_x${NR}__neon(
  size_t g,
  size_t nc,
  size_t kc,
  size_t nr,
  size_t kr,
  size_t sr,
  const uint32_t* weights,
  const uint32_t* bias,
  uint32_t* packed_weights,
  size_t extra_bytes,
  const void* params)
{
  assert(g != 0);
  assert(nc != 0);
  assert(kc != 0);
  assert(nr == ${NR});   // This kernel is for NR=${NR}
  assert(kr == 1);
  assert(sr == 1);
  assert(weights != NULL);
  assert(packed_weights != NULL);

  $for N in range(0,NR,4):
    $for K in range(0,KUNROLL,4):
      uint32x4x4_t v${N}${K};
      $for NN in range(0,4):
        v${N}${K}.val[${NN}] = vdupq_n_u32(0);

  $for N in range(NR):
    const uint32_t* w${N};

  uint32_t* zerobuf = (uint32_t*) alloca(kc * sizeof(uint32_t));
  memset(zerobuf, 0, kc * sizeof(uint32_t));

  do {
    // NC main loop multiple of ${NR}
    const uint32_t* w = weights;
    size_t n = nc;

    do {
      $for R in range(0,2):
        $if R == 0:
          if (n >= ${NR}) {
            if XNN_LIKELY(bias != NULL) {
              $for N in range(0,NR,4):
                uint32x4_t vb${N} = vld1q_u32(bias + ${N});
              $for N in range(0,NR,4):
                vst1q_u32(packed_weights + ${N}, vb${N});
              bias += ${NR};
            }
            packed_weights += ${NR};

            w0 = w;
            $for N in range(1, NR):
              w${N} = w${N-1} + kc;
        $else:
          else {
            // NC remainder (1..${NR-1})
            if XNN_LIKELY(bias != NULL) {
              size_t nb = n;
              do {
                *packed_weights++  = *bias++;
              } while (--nb != 0);
              packed_weights += (${NR} - n);
            } else {
              packed_weights += ${NR};
            }

            w0 = w;
            $for N in range(1, NR):
              w${N} = (n > ${N}) ? (w${N-1} + kc) : zerobuf;

        // KC main loop multiple of ${NR}x${KUNROLL}
        size_t k = kc;
        for (; k >= ${KUNROLL}; k -= ${KUNROLL}) {
          $for N in range(0,NR,4):
            $for NN in range(0,4):
              v${N}0 = vld4q_lane_u32(w${N+NN}, v${N}0, ${NN});
              $for K in range(4,KUNROLL,4):
                v${N}${K} = vld4q_lane_u32(w${N+NN} + ${K}, v${N}${K}, ${NN});
              w${N+NN} += ${KUNROLL};
          $for K in range(0,KUNROLL,4):
            $for NN in range(0,4):
              $for N in range(0,NR,4):
                vst1q_u32(packed_weights + ${N+NN*NR+K*KUNROLL}, v${N}${K}.val[${NN}]);
          packed_weights += ${NR*KUNROLL};
        }

        // KC remainder of 1..${KUNROLL-1}
        // Same as main loop but ld1, ld2 or ld3
        if XNN_UNLIKELY(k != 0) {
          $if KUNROLL == 4:
            switch (k) {
              $for K in range(1,4):
                // KC remainder of ${NR}x${K}
                case ${K}:
                {
                  $if K == 1:
                    $for N in range(0,NR,4):
                      uint32x4_t v${N} = vdupq_n_u32(0);
                  $else:
                    $for N in range(0,NR,4):
                      uint32x4x${K}_t v${N};
                      $for NN in range(0,K):
                        v${N}.val[${NN}] = vdupq_n_u32(0);

                  $for N in range(0,NR,4):
                    $for NN in range(0,4):
                      v${N} = vld${K}q_lane_u32(w${N+NN}, v${N}, ${NN});
                      w${N+NN} += ${K};

                  $for NN in range(0,K):
                    $for N in range(0,NR,4):
                      $if K == 1:
                        vst1q_u32(packed_weights + ${N+NN*NR}, v${N});
                      $else:
                        vst1q_u32(packed_weights + ${N+NN*NR}, v${N}.val[${NN}]);
                  packed_weights += ${NR*K};
                  break;
                }
              default:
                XNN_UNREACHABLE;
            }
          $else:
            for (; k != 0; --k) {
              $for N in range(0,NR,4):
                $for NN in range(0,4):
                  $for K in range(0,KUNROLL,4):
                    v${N}${K}.val[0] = vld1q_lane_u32(w${N+NN} + ${K}, v${N}${K}.val[0], ${NN});
                  w${N+NN} += ${KUNROLL//4};
              $for K in range(0,KUNROLL,4):
                $for N in range(0,NR,4):
                  vst1q_u32(packed_weights + ${N+K*KUNROLL}, v${N}${K}.val[0]);
              packed_weights += ${NR*KUNROLL//4};
            }
        }
        packed_weights = (uint32_t*) ((uintptr_t) packed_weights + extra_bytes);

        $if R == 0:
          w = w${NR-1};
          n -= ${NR};
        $else:
          w += (n - 1) * kc;
          n = 0;
        }
    } while (n != 0);

    weights += nc * kc;
  } while (--g != 0);
}
