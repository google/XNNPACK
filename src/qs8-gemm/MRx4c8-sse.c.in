    ${_}}
    $if DATATYPE == "QB4":
      ${_}// accumulate float
      $if SSE == 4:
        ${_}const __m128 vfilter_output_scale0123 = _mm_castsi128_ps(_mm_slli_epi32(_mm_cvtepu16_epi32(_mm_loadl_epi64((const __m128i*) w)), 16));
      $else:
